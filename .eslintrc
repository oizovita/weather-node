{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],

  "rules": {
    "no-console": "off",
    "no-plusplus": "off",
    "max-len": ["error", 150,{"ignoreUrls": true}],
    "no-tabs": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "indent": [2, "tab"],
    "no-restricted-syntax":"off",
    "func-names":"off"
  }
}